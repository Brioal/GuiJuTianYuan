<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.brioal.guijutianyuan.activity.MainActivity">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/main_container"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="@color/colorWhite">

        </RelativeLayout>

        <com.brioal.guijutianyuan.view.view.ButtomTabLayout
            android:id="@+id/bottom_tablayout"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_gravity="bottom"
            app:layout_behavior="com.brioal.brioallibrary.mdtablayout.view.behavior.TabLayoutBehavior">


        </com.brioal.guijutianyuan.view.view.ButtomTabLayout>

    </LinearLayout>
</RelativeLayout>
